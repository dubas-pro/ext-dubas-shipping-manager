{
    "fields": {
        "name": {
            "type": "number",
            "readOnly": true,
            "unique": true,
            "nextNumber": 1,
            "padLength": 8,
            "prefix": "1"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": true,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "weight": {
            "notNull": false,
            "type": "float",
            "min": 1,
            "isCustom": true
        },
        "parcelType": {
            "type": "enum",
            "options": [
                "Delivery",
                "Pickup"
            ],
            "style": {
                "Delivery": null,
                "Pickup": null
            },
            "default": "Delivery",
            "tooltip": true,
            "isCustom": true,
            "required": true
        },
        "deliveryAddress": {
            "type": "address",
            "isCustom": true
        },
        "pickupAddress": {
            "type": "address",
            "isCustom": true
        },
        "height": {
            "notNull": false,
            "type": "float",
            "min": 1,
            "isCustom": true
        },
        "lenght": {
            "notNull": false,
            "type": "float",
            "min": 1,
            "isCustom": true
        },
        "width": {
            "notNull": false,
            "type": "float",
            "min": 1,
            "isCustom": true
        },
        "senderContact": {
            "type": "link"
        },
        "recipientContact": {
            "type": "link"
        },
        "sender": {
            "type": "link"
        },
        "recipient": {
            "type": "link"
        },
        "parcelStatuses": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "isCustom": true
        },
        "pickupLocation": {
            "type": "link",
            "view": "dubas-shipping-manager:views/dubas-parcel/fields/pickup-location"
        },
        "deliveryLocation": {
            "type": "link",
            "view": "dubas-shipping-manager:views/dubas-parcel/fields/delivery-location"
        },
        "deliveryInstructions": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "pickupInstructions": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "price": {
            "type": "currency",
            "isCustom": true
        },
        "pickupDate": {
            "notNull": false,
            "type": "date",
            "readOnly": true,
            "isCustom": true
        },
        "deliveryDate": {
            "notNull": false,
            "type": "date",
            "readOnly": true,
            "isCustom": true
        },
        "packages": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "isCustom": true
        },
        "referenceNumber": {
            "type": "varchar",
            "maxLength": 150,
            "trim": true,
            "options": [],
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "EntityTeam",
            "layoutRelationshipsDisabled": true
        },
        "senderContact": {
            "type": "belongsTo",
            "foreign": "sentParcels",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        },
        "recipientContact": {
            "type": "belongsTo",
            "foreign": "receivedParcels",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        },
        "sender": {
            "type": "belongsTo",
            "foreign": "sentParcels",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "recipient": {
            "type": "belongsTo",
            "foreign": "receivedParcels",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "parcelStatuses": {
            "type": "hasMany",
            "foreign": "parcel",
            "entity": "DubasParcelStatus",
            "audited": false,
            "isCustom": true
        },
        "pickupLocation": {
            "type": "belongsTo",
            "foreign": "sentParcels",
            "entity": "DubasLocation",
            "audited": false,
            "isCustom": true
        },
        "deliveryLocation": {
            "type": "belongsTo",
            "foreign": "deliveredParcels",
            "entity": "DubasLocation",
            "audited": false,
            "isCustom": true
        },
        "packages": {
            "type": "hasMany",
            "foreign": "parcel",
            "entity": "DubasPackage",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        }
    }
}