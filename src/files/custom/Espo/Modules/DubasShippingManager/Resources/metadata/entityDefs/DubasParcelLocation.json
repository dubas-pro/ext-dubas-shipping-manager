{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "trim": true
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": true,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "contact": {
            "type": "link"
        },
        "instructions": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "typeOfLocation": {
            "type": "multiEnum",
            "storeArrayValues": true,
            "required": true,
            "options": [
                "Pickup",
                "Delivery",
                "Pickup Point"
            ],
            "style": {
                "Pickup": null,
                "Delivery": null,
                "Pickup Point": null
            },
            "isCustom": true
        },
        "address": {
            "type": "address",
            "viewMap": true,
            "isCustom": true
        },
        "status": {
            "type": "enum",
            "options": [
                "Available",
                "Unavailable"
            ],
            "style": {
                "Available": null,
                "Unavailable": null
            },
            "audited": true,
            "default": "Available",
            "isCustom": true
        },
        "number": {
            "type": "number",
            "len": 36,
            "notNull": false,
            "unique": false,
            "nextNumber": 1,
            "padLength": 5,
            "prefix": "L-",
            "isCustom": true
        },
        "account": {
            "type": "link"
        },
        "sentParcels": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "isCustom": true
        },
        "deliveredParcels": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "EntityTeam",
            "layoutRelationshipsDisabled": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "dubasParcelLocations",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        },
        "account": {
            "type": "belongsTo",
            "foreign": "dubasParcelLocations",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "sentParcels": {
            "type": "hasMany",
            "foreign": "pickupLocation",
            "entity": "DubasParcel",
            "audited": false,
            "isCustom": true
        },
        "deliveredParcels": {
            "type": "hasMany",
            "foreign": "deliveryLocation",
            "entity": "DubasParcel",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        }
    }
}